{
  "name": "dewinternet/craft",
  "description": "A starter Craft CMS installation for DEW Websites",
  "version": "1.0.0",
  "type": "project",
  "require": {
    "craftcms/aws-s3": "1.2.5",
    "craftcms/cms": "^3.0.0",
    "craftcms/feed-me": "^4.1.2",
    "craftcms/redactor": "^2.3.3.2",
    "nystudio107/craft-imageoptimize": "^1.6",
    "nystudio107/craft-minify": "^1.2.5",
    "nystudio107/craft-retour": "^3.0.0",
    "nystudio107/craft-routemap": "1.1.6",
    "nystudio107/craft-seomatic": "^3.0.0",
    "nystudio107/craft-twigpack": "^1.0.0",
    "nystudio107/craft-typogrify": "^1.1.1",
    "studioespresso/craft-dumper": "^2.0.0",
    "vlucas/phpdotenv": "^3.4.0",
    "yiisoft/yii2-redis": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
